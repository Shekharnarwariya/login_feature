type Query {
    viewKeyword(id:String): KeywordEntry
}

type KeywordEntry {
    id: Int
    userId: Int
    prefix: String
    suffix: String
    type: String
    shortCode: String
    expiresOn: String
    reply: boolean
    replyMessage: String
    replyOnFailed: String
    replyOnExpire: String
    replySender: String
    alertNumber: String
    alertEmail: String
    alertUrl: String
    createdOn: String
    createdBy: String
    sources: String
    systemId:String
}
